<Alloy>
  <View id="container" onTouchstart="focused">
    <View id="view" onClick="togglePicker">
      <Label id="form_label" class="form_label"/>
      <Label id="form_picker_label" class="form_picker_label"/>
    </View>
    <View id="drawer" platform="ios" layout="vertical">
      <Picker id="picker" onChange="pickerChange">
        <PickerColumn id="pickerColumn"/>
      </Picker>
    </View>
  </View>
</Alloy>
